function barragebuff()
	if activu == true then return end
	if appeared == true then
		return
	end
	trans0()
	activu = true
	local bep = true
	local de = mouse.KeyUp:connect(function(key)
	    if key == "j" then
	      bep = false
	    end
	end)
	trailevent:FireServer(rarm.Trail, false)
	trailevent:FireServer(larm.Trail, false)	
	barrageanim:Play(0.1, 2, 8)
	
	repeat
		wait(0.025)
		hito6(rarm, rarm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://2894385265", math.random(9, 11) / 10, math.random(9, 11) / 50)
		hito6(larm, larm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://2894385265", math.random(9, 11) / 10, math.random(9, 11) / 50)	
		hito6(rarm, rarm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://2894385265", math.random(9, 11) / 10, math.random(9, 11) / 50)
		hito6(larm, larm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://2894385265", math.random(9, 11) / 10, math.random(9, 11) / 50)	
		hito6(rarm, rarm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://1837829565", math.random(9, 11) / 10, math.random(9, 11) / 50)
		hito6(larm, larm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://1837829565", math.random(9, 11) / 10, math.random(9, 11) / 50)	
		hito6(rarm, rarm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://1837829565", math.random(9, 11) / 10, math.random(9, 11) / 50)
		hito6(larm, larm.CFrame * CFrame.new(0, -5, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,2, 65, 0.025, 0.05, rot.CFrame.lookVector * 5, 0.075,"rbxassetid://1837829565", math.random(9, 11) / 10, math.random(9, 11) / 50)	
	until bep == false
	trans1()
	barrageanim:Stop(0.1)
	trailevent:FireServer(rarm.Trail, false)
	trailevent:FireServer(larm.Trail, false)
	activu = false
end

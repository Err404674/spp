local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
	Name = "ABDM Hub",
	LoadingTitle = "ku klux klan on top",
	LoadingSubtitle = "made by uhh i forgot",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = "Rayfield Interface Suite",
		FileName = "Big Hub"
	},
	KeySystem = true, -- Set this to true to use their key system
	KeySettings = {
		Title = "r",
		Subtitle = "Key System",
		Note = "Key: femboysimulator",
		SaveKey = true,
		Key = "femboysimulator"
	}
})

Rayfield:Notify("Title Example", "Content/Description Example", 4483362458) -- Notfication -- Title, Content, Image

local Tab = Window:CreateTab("Scripts", 4483362458) -- Title, Image



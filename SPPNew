            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 0.7, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 0.7, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 0.7, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 0.7, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 0.7, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 94.999, 0, Vector3.new(0, 0, 0), 1, "", 1, 1)
            hitbox1:Destroy()
            end
        end
    end)

mouse.KeyDown:connect(function(key)
	if key == "j" then
    if active == true then return end
		active = true
		_G.anim30:Play()
		wait(0.6)
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-1,-0.1,-1), Vector3.new(-1,-0.1,-1), BrickColor.new("Bright green"))
		_G.hoomanR4()
		wait(0.5)
		active = false
	end
end)

Uppercutcd = false
function Uppercut()
	if Uppercutcd == true then return end
	if activu == true then return end
	Uppercutcd = true
	activu = true
	if appeared == true then
		return
	end
	trans0()
	human.JumpPower = 0
	_G.anim212:Play(0.1, 1, 1)
--	trailevent:FireServer(rarm.Trail, true)
	strongpunchevent:FireServer(Vector3.new(.5, 0.5, .5), Vector3.new(0.3, 0.15, 0.3), BrickColor.new("Black"))
	coroutine.resume(coroutine.create(function()
		for _ = 1, 15 do
			wait()
			hito54(rarm, rarm.CFrame * CFrame.new(0, -1, 0) * CFrame.new(math.random(-0.25, 0.25), math.random(-0.25, 0.25), math.random(-0.25, 0.25)) ,3, 65, 0.75, 0.25, rot.CFrame.lookVector * 50 + Vector3.new(0, 25, 0), "rbxassetid://8595977193", 0.075, Color3.new(255, 255, 255),"rbxassetid://260430079", math.random(9, 11) / 10, math.random(9, 11) / 35, true)
		end
	end))
	wait(0.65)
	_G.anim212:Stop()
--	trailevent:FireServer(rarm.Trail, false)
	human.JumpPower = 50
	trans1()
	activu = false	
	wait()
	Uppercutcd = false
end
